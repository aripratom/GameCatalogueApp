<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.aripratom.gamecatalogue.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
    </fragment>
    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.aripratom.favorite.FavoriteFragment"
        app:moduleName="Game_Catalogue.favorite"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite" >

    </fragment>
    <fragment
        android:id="@+id/navigation_search"
        android:name="com.aripratom.gamecatalogue.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >

    </fragment>
</navigation>